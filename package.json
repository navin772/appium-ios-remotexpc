{
  "name": "appium-ios-remotexpc",
  "version": "0.0.1",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/src/index.js",
      "types": "./build/src/index.d.ts"
    }
  },
  "engines": {
    "node": ">=20 <23"
  },
  "scripts": {
    "clean:build": "rimraf ./build",
    "build:es": "tsc",
    "build": "run-s clean:* build:*",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"{src,test}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "sudo mocha test/integration/**/*.ts",
    "test:all": "sudo mocha -r tsx/cjs test/run-integration-tests.ts",
    "test:tunnel": "sudo mocha test/integration/tunnel-test.ts --exit --timeout 1m",
    "test:pair-record": "sudo mocha test/integration/read-pair-record-test.ts --exit --timeout 1m",
    "test:diagnostics": "sudo mocha test/integration/diagnostics-test.ts --exit --timeout 1m",
    "test:unit": "mocha test/unit/**/*.ts --exit --timeout 2m"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@appium/tsconfig": "^0.3.5",
    "@eslint/js": "^9.23.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/chai": "^5.2.1",
    "@types/chai-as-promised": "^8.0.2",
    "@types/mocha": "^10.0.10",
    "appium": "^2.17.1",
    "chai": "^5.2.0",
    "chai-as-promised": "^8.0.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-unicorn": "^58.0.0",
    "mocha": "^11.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "tuntap-bridge": "^0.0.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.29.0"
  },
  "dependencies": {
    "@types/node": "^22.14.0",
    "@xmldom/xmldom": "^0.9.8",
    "npm-run-all2": "^7.0.2"
  }
}
